{
  "paths": {
    "definitions_dir": "../../services/business_services/analytics_metadata_service/definitions",
    "kpis_dir": "../../services/business_services/analytics_metadata_service/definitions/kpis",
    "objects_dir": "../../services/business_services/analytics_metadata_service/definitions/object_models",
    "modules_dir": "../../services/business_services/analytics_metadata_service/definitions/modules",
    "value_chains_dir": "../../services/business_services/analytics_metadata_service/definitions/value_chains",
    "output_dir": "./output",
    "backup_dir": "./backups",
    "logs_dir": "./logs"
  },
  "sync_options": {
    "create_backups": true,
    "dry_run": false,
    "batch_size": 50,
    "max_related_kpis": 30,
    "max_related_objects": 20,
    "max_key_attributes": 20
  },
  "validation": {
    "check_missing_references": true,
    "check_orphaned_objects": true,
    "check_circular_dependencies": true,
    "check_formatting": true,
    "fail_on_errors": false
  },
  "logging": {
    "level": "INFO",
    "console_output": true,
    "file_output": true,
    "max_log_files": 10
  },
  "reporting": {
    "generate_html": true,
    "generate_markdown": true,
    "include_charts": true,
    "email_report": false
  }
}
